{
   "name": "Home-Findr",
   "version": "1.0",
   "description": "An extension to add listings to your Home-Findr account.",
   "manifest_version": 2,
   "browser_action": {
     "default_icon": "favehome.png",
     "default_popup": "index.html"
   },
   "background": {
     "scripts": ["background.js"],
     "persistent": false
   },
   "permissions": [
      "identity",
      "identity.email",
      "cookies",
      "tabs",
      "http://www.streeteasy.com/",
      "https://www.streateasy.com/",
      "http://www.nakedapartments.com/"
      "https://www.nakedapartments.com/"
    ]
 }
